{
    "name": "art-es/laravel-builder",
    "license": "MIT",
    "description": "Package for creating builders for your models from laravel framework",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "authors": [
        {
            "name": "Arthur Shaidullin",
            "email": "sh.artur.ilf@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Artes\\LaravelBuilder\\": "src/LaravelBuilder/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artes\\LaravelBuilder\\LaravelBuilderServiceProvider"
            ]
        }
    }
}
